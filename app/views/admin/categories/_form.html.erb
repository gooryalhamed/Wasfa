<div class="container wrapper">
	<div id="title">
		<span>CATEGORY / FORMS</span>
		<i class="fa fa-wpforms" aria-hidden="true"></i>
	</div>
	
	<div id="form-wrapper">
		<%= form_for([:admin, @category]) do |f|%>
		<div id="field">
			<%= f.label :name %>
			<%= f.text_field :name %>
		</div>
		<%= f.submit :class => "btn" %>
		<% end %>
		<%= link_to 'Back', admin_categories_path,{:class => "btn"} %>
	</div>
</div>