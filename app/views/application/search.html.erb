<div class="container" id="search-container">
	<h4>We found <%= @results.length %> results for your search</h4>
	<div class="row">
		<% @results.each do |res| %>
		<div class="row">
			<div class="col-sm-4 col-xs-5">
				<%= image_tag("/uploads/#{res.image}") %>
			</div>
			<div class="col-xs-3" id="recName">
				<%= link_to "#{res.name}", food_recipe_path(res) %>
			</div>
		</div>
		<% end %>
	</div>
</div>
